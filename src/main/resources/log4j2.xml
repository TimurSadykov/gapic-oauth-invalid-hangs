<Configuration>
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_ERR">
      <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n %ex"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="all">
      <AppenderRef ref="CONSOLE"/>
    </Root>

    <Logger name="com.google.ads.googleads" level="all"/>
  </Loggers>
</Configuration>

